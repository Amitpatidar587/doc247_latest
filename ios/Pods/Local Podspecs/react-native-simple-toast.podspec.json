{
  "name": "react-native-simple-toast",
  "version": "3.3.2",
  "summary": "Cross-platform Toast experience for React Native",
  "homepage": "https://github.com/vonovak/react-native-simple-toast#readme",
  "license": "MIT",
  "authors": "Vojtech Novak <vonovak@gmail.com> (https://github.com/vonovak)",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/vonovak/react-native-simple-toast.git",
    "tag": "3.3.2"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "dependencies": {
    "Toast": [
      "~> 4"
    ],
    "React-Core": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "React-hermes": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  },
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  }
}
